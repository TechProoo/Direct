    "target": "ES6",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "outDir": "dist",
    "noEmit": false,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "resolveJsonModule": true
